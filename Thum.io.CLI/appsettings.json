{
	"Serilog": {
		"MinimumLevel": "Debug",
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level:w3}] {Message:lj}{NewLine}{Exception}"
				}
			},
			{
				"Name": "File",
				"Args": {
					"path": "%UserProfile%\\.thum.io\\cli.log",
					"outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level:w3}] {Message:lj}{NewLine}{Exception}"
				}
			}
		]
	}
}